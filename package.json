{
  "name": "adonis-web-starter-kit",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --watch",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck:adonis": "tsc --noEmit -p tsconfig.json",
    "typecheck:vue": "vue-tsc --noEmit -p inertia/tsconfig.json",
    "typecheck": "npm run typecheck:adonis && npm run typecheck:vue"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.4.0",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.2",
    "@swc/core": "^1.4.13",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.0",
    "@adonisjs/core": "^6.5.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/inertia": "^1.0.0-25",
    "@adonisjs/lucid": "^20.5.1",
    "@adonisjs/session": "^7.3.0",
    "@adonisjs/shield": "^8.1.1",
    "@adonisjs/static": "^1.1.1",
    "@adonisjs/vite": "^3.0.0-11",
    "@inertiajs/vue3": "^1.0.15",
    "@vinejs/vine": "^1.8.0",
    "edge.js": "^6.0.2",
    "luxon": "^3.4.4",
    "mysql2": "^3.9.4",
    "reflect-metadata": "^0.2.2",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.13"
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
}